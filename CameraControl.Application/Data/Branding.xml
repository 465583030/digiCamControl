<?xml version="1.0"?>
<Branding xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ApplicationTitle>digiCamControl</ApplicationTitle>
  <!-- full path to startup screen image --> 
  <StartupScreenImage></StartupScreenImage>
  <LogoImage></LogoImage>
  <DefaultThumbImage></DefaultThumbImage>
  <DefaultMissingThumbImage />
  <!-- hide all link buttons to DCC home page --> 
  <OnlineReference>false</OnlineReference>
   <!-- true and false are case sensitive  -->  
  <ShowStartupScreen>false</ShowStartupScreen>
  <CheckForUpdate>false</CheckForUpdate>
   <!-- main screen top bar button   -->  
  <ShowRefreshButtonMainWindow>false</ShowRefreshButtonMainWindow>
  <ShowDownloadButtonMainWindow>false</ShowDownloadButtonMainWindow>
  <ShowBracketingButtonMainWindow>false</ShowBracketingButtonMainWindow>
  <ShowTimelapseButtonMainWindow>false</ShowTimelapseButtonMainWindow>
  <ShowFullScreenButtonMainWindow>false</ShowFullScreenButtonMainWindow>
  <ShowLiveViewButtonMainWindow>false</ShowLiveViewButtonMainWindow>
  <ShowBrowseButtonMainWindow>false</ShowBrowseButtonMainWindow>
  <ShowTagsButtonMainWindow>false</ShowTagsButtonMainWindow>
  <ShowAstronomyButtonMainWindow>false</ShowAstronomyButtonMainWindow>
  <ShowMulyiCameraButtonMainWindow>false</ShowMulyiCameraButtonMainWindow>
  <ShowWifiCameraButtonMainWindow>false</ShowWifiCameraButtonMainWindow>
  <ShowPrintButtonMainWindow>false</ShowPrintButtonMainWindow>
  
  <ShowSettingMenuMainWindow>false</ShowSettingMenuMainWindow>
  <ShowMenuMenuMainWindow>false</ShowMenuMenuMainWindow>
  <ShowCameraSelectorMainWindow>false</ShowCameraSelectorMainWindow>
  <ShowRightPanelMainWindow>true</ShowRightPanelMainWindow>
  <ShowCameraPropertiesMainWindow>false</ShowCameraPropertiesMainWindow>
  <!-- Main window right panel customization  -->  
  <ShowHistogram>false</ShowHistogram>
  <ShowSelectionPanel>false</ShowSelectionPanel>
  <ShowLayoutPanel>false</ShowLayoutPanel>
  <ShowSessionPanel>true</ShowSessionPanel>
  <ShowAutoExportPanel>false</ShowAutoExportPanel>
  <ShowPresetPanel>false</ShowPresetPanel>  
  <ShowImagePropertiesPanel>false</ShowImagePropertiesPanel>
  <ShowMetadataPanel>false</ShowMetadataPanel>
  <ShowExportPanel>false</ShowExportPanel>
  <ShowToolsPanel>false</ShowToolsPanel>
   <!-- live view left panel group boxes  -->  
  <ShowFocusControlLiveView>true</ShowFocusControlLiveView>
  <ShowFocusStackingLiveView>true</ShowFocusStackingLiveView>
  <ShowControlLiveView>true</ShowControlLiveView>
  <ShowOverlayLiveView>true</ShowOverlayLiveView>
  <ShowDisplayLiveView>true</ShowDisplayLiveView>
  <ShowMotionDetectionLiveView>true</ShowMotionDetectionLiveView>
  <!-- show animation when a camera is selected-->
  <ShowAnimation>false</ShowAnimation>
 
</Branding>